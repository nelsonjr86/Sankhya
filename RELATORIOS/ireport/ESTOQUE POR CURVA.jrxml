	<queryString>
		<![CDATA[SELECT DISTINCT P.CODPROD, P.REFERENCIA, P.DESCRPROD, G.CURVA, G.CODEMP, EM.NOMEFANTASIA
FROM TGFPRO P INNER JOIN AD_CURVA_GIRO G ON (P.CODPROD=G.CODPROD)
			  INNER JOIN TGFEST E ON (E.CODPROD=P.CODPROD AND G.CODEMP = E.CODEMP)
			  INNER JOIN TSIEMP EM ON (E.CODEMP=EM.CODEMP)
WHERE G.CODEMP = $P{CODEMP} AND
G.CURVA = $P{CURVA} AND
P.USOPROD = 'R'
AND E.ESTOQUE > 0
ORDER BY P.REFERENCIA]]>
	</queryString>
