	<queryString>
		<![CDATA[SELECT DISTINCT P.CODPROD, P.REFERENCIA, P.DESCRPROD, G.CURVA, G.CODEMP, EM.NOMEFANTASIA
FROM TGFPRO P INNER JOIN TGFITE I ON (I.CODPROD=P.CODPROD)
			  INNER JOIN TGFCAB C ON (I.NUNOTA=C.NUNOTA)
			  INNER JOIN AD_CURVA_GIRO G ON (I.CODPROD=G.CODPROD AND G.CODEMP=I.CODEMP)
			  INNER JOIN TGFEST E ON (E.CODPROD=P.CODPROD AND G.CODEMP = E.CODEMP)
			  INNER JOIN TSIEMP EM ON (C.CODEMP=EM.CODEMP)
WHERE C.CODEMP = $P{CODEMP} AND
P.USOPROD = 'R' AND
C.TIPMOV = 'C' AND
E.ESTOQUE > 0 AND
C.CODTIPOPER NOT IN (708,787) AND
 G.CURVA = $P{CURVA} AND
C.DTENTSAI BETWEEN $P{DTINI} AND $P{DTFIN}
ORDER BY G.CURVA, P.REFERENCIA]]>
	</queryString>
