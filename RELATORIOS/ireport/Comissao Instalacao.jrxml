	</parameter>
	<queryString>
		<![CDATA[SELECT CODVEND, NOME, VLRUNITINS, QTDINS, VLRUNITINS * QTDINS AS VLRTOTINS
FROM (
SELECT TGFVEN.CODVEND, TGFVEN.APELIDO AS NOME,
ISNULL(TGFVEN.AD_VLRCOMISSAOINSTALACAO,0) AS VLRUNITINS,
SUM(CASE WHEN AD_PRODINS.CODINSTA1 = TGFVEN.CODVEND OR AD_PRODINS.CODINSTA2 = TGFVEN.CODVEND OR AD_PRODINS.CODINSTA3 = TGFVEN.CODVEND THEN 1 ELSE 0 END) AS QTDINS
FROM TGFVEN, AD_INSPRO, AD_PRODINS
WHERE AD_INSPRO.CODINS = AD_PRODINS.CODINS
AND TGFVEN.TIPVEND = 'T'
AND AD_PRODINS.DTINSTA BETWEEN $P{P_DTI} AND $P{P_DTF}
GROUP BY TGFVEN.CODVEND, TGFVEN.APELIDO, TGFVEN.AD_VLRCOMISSAOINSTALACAO
) CONSULTA
ORDER BY CODVEND]]>
	</queryString>
